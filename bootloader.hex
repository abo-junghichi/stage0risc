# bootloader in the rom.
50 00 00 00 # program size
# start:0x00
# read 4 byte header
80 01 01 00 # lit %1 $1
80 08 08 00 # lit %8 $8
a1 0a 00 00 # getc %a
a1 0b 00 00 # getc %b
a1 0c 00 00 # getc %c
a1 0d 00 00 # getc %d
8d 0b 0b 08 # shl %b %b %8
89 0a 0a 0b # or %a %a %b
8d 0d 0d 08 # shl %d %d %8
89 0c 0c 0d # or %c %c %d
80 fe 10 00 # lit %fe $16
8d 0c 0c ff # shl %c %c %ff
89 0a 0a 0c # or %a %a %c
81 0d 1c 00 # rel %d @data # @data = 0x1c
82 ff 14 00 # jal %ff @branch # @branch = 0x14
# loop:0x40
a1 0c 00 00 # getc %c
93 0c 00 0d # sb %c $0 %d
86 0d 0d 01 # add %d %d %1
87 0a 0a 01 # sub %a %a %1
#branch:0x50
85 0a f0 ff # bnez %a @loop # @loop = -0x10 = 0xfff0
# data:0x54
$
